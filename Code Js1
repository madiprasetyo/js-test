/** 
* Direction: 
* Swap these numbers without using temporary variable 
* 
* Expected Result: 
* { 
* a: 4, 
* b: 2 
* } 
*/

let numbers = { a: 2, b: 4 }; 

function result(numbers) {

 //my Code Here
 //swab, the number
 [numbers.a, numbers.b] = [numbers.b, numbers.a] ;
 

 //to create the same, output as expected
 return "{" + "\n" + "a: " + numbers.a + "," + "\n" + "b: " + numbers.b + "\n" + "}";
 }

console.log(result(numbers));
